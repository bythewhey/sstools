echo ===== Recently created DLL files ===== > DLLActivity.txt && fsutil usn readjournal C: csv | findstr /i /c:".dll" | findstr /i /c:0x00000100 >> DLLActivity.txt && echo. >> DLLActivity.txt && echo ===== Recently deleted DLL files ===== >> DLLActivity.txt && fsutil usn readjournal C: csv | findstr /i /c:".dll" | findstr /i /c:0x80000200 >> DLLActivity.txt && echo. >> DLLActivity.txt && echo ===== Recently renamed DLL files ===== >> DLLActivity.txt && fsutil usn readjournal C: csv | findstr /i /c:".dll" | findstr /i /c:0x00001000 >> DLLActivity.txt && echo. >> DLLActivity.txt && echo ===== Previously renamed DLL files ===== >> DLLActivity.txt && fsutil usn readjournal C: csv | findstr /i /c:".dll" | findstr /i /c:0x00002000 >> DLLActivity.txt && notepad DLLActivity.txt
