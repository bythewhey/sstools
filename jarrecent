echo ===== Recently created JAR files ===== > JARActivity.txt && fsutil usn readjournal C: csv | findstr /i /c:".jar" | findstr /i /c:0x00000100 >> JARActivity.txt && echo. >> JARActivity.txt && echo ===== Recently deleted JAR files ===== >> JARActivity.txt && fsutil usn readjournal C: csv | findstr /i /c:".jar" | findstr /i /c:0x80000200 >> JARActivity.txt && echo. >> JARActivity.txt && echo ===== Recently renamed JAR files ===== >> JARActivity.txt && fsutil usn readjournal C: csv | findstr /i /c:".jar" | findstr /i /c:0x00001000 >> JARActivity.txt && echo. >> JARActivity.txt && echo ===== Previously renamed JAR files ===== >> JARActivity.txt && fsutil usn readjournal C: csv | findstr /i /c:".jar" | findstr /i /c:0x00002000 >> JARActivity.txt && notepad JARActivity.txt
